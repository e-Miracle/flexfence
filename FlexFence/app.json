{
  "expo": {
    "name": "FlexFence",
    "slug": "FlexFence",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/purplelogo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./src/assets/purplelogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera for selfie verification."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/purplelogo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "CAMERA",
        "CAMERA_ROLL"
      ],
      "package": "com.flexfence.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
